{
  "name": "spun",
  "version": "0.3.0",
  "description": "Rapid specification authoring for Selenium.",
  "author": "Joseph Spencer",
  "preferGlobal": true,
  "scripts": {
    "pretest": "jshint lib && jshint --config ./test/.jshintrc test",
    "cover": "istanbul cover _mocha && cat ./coverage/lcov.info | coveralls -v && rm -rf coverage",
    "test": "make test"
  },
  "bin": {
    "spun": "./bin/spun.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsdevel/spun.git"
  },
  "keywords": [
    "selenium",
    "spec",
    "runner"
  ],
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsdevel/spun/issues"
  },
  "devDependencies": {
    "coveralls": "~2.10.0",
    "should": "~4.0.4",
    "mocha": "~1.20.1",
    "istanbul": "~0.2.13",
    "jshint": "~2.5.1",
    "composites": "*",
    "sinon": "~1.10.2"
  },
  "dependencies": {
    "glob": "^4.0.2",
    "minimist": "^0.2.0",
    "chalk": "^0.4.0",
    "findup-sync": "^0.1.3",
    "is-dir": "^1.0.0",
    "async": "^0.9.0",
    "err-handler": "^0.0.0",
    "spun-util": "^0.4.0",
    "dir-to-exports": "^1.0.1",
    "is-file": "~1.0.0",
    "once": "~1.3.0",
    "event-stream": "~3.1.5",
    "spun-webdriver-sync": "^0.1.0"
  }
}
